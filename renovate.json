{
  "extends": [
    "config:recommended"
  ],
  "prCreation": "immediate",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/(^|/)compose\\.ya?ml$/",
        "/(^|/)docker-compose\\.ya?ml$/"
      ],
      "matchStrings": [
        "image:\\s*(?<depName>[^\\s:@]+)(?::(?<currentValue>[^\\s@]+))?"
      ],
      "datasourceTemplate": "docker"
    }
  ],
  "assignees": [
    "scottfridwin"
  ]
}
